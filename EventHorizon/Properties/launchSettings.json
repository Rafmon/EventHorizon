{
  "profiles": {
    "http": {
      "commandName": "Project",
      "commandLineArgs": "--simulateI2C",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:80/",
      "dotnetRunMessages": true
    },
    "https": {
      "commandName": "Project",
      "commandLineArgs": "--simulateI2C",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:443/"
    },


    "Docker Compose": {
      "commandName": "DockerCompose",
      "commandVersion": "1.0",
      "serviceActions": {
        "eventhorizon": "StartDebugging"
      }
    },


    "iisSettings": {
      "windowsAuthentication": false,
      "anonymousAuthentication": true,
      "iisExpress": {
        "applicationUrl": "http://localhost:50595/",
        "sslPort": 44316
      }
    }
  }
